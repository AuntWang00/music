<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="music_customer" catalog="music"
		class="com.music.model.Music_customer">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="music_order" catalog="music"
		class="com.music.model.Music_order">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="songs" catalog="music" class="com.music.model.Songs">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>